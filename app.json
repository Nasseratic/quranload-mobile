{
  "expo": {
    "name": "quranload",
    "slug": "quranload",
    "version": "1.5.0",
    "runtimeVersion": "1.5.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.quranload.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.quranloadapp.quranload",
      "blockedPermissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO"
      ],
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [ 
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "organization": "qlmo",
          "project": "react-native",
          "authToken": false,
          "setCommits": true
        }
      ],
      [
        "@stripe/stripe-react-native",
        {}
      ],
      "expo-font",
      "expo-localization",
      [
        "expo-audio",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone to be able to record your recitation, submit feedback or send a voice note in the chat."
        }
      ],
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "0560d09e-9d38-44d5-b53c-743b6488c3a1"
      }
    },
    "owner": "quranload-app",
    "updates": {
      "fallbackToCacheTimeout": 5000,
      "requestHeaders": {
        "expo-channel-name": "production"
      },
      "url": "https://u.expo.dev/0560d09e-9d38-44d5-b53c-743b6488c3a1"
    }
  }
}
