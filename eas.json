{
  "cli": {
    "version": ">= 4.1.2",
    "appVersionSource": "remote"
  },
  "build": {
    "dev": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev"
    },
    "dev-sim": {
      "extends": "dev",
      "ios": {
        "simulator": true
      },
      "channel": "dev-simulator"
    },
    "preview": {
      "distribution": "internal",
      "channel": "production"
    },
    "prod": {
      "channel": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "prod": {
      "ios": {
        "companyName": "Wemba ApS"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
